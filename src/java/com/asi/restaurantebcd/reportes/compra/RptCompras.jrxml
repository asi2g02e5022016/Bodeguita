	<queryString>
		<![CDATA[select
 enca.idcompra,
enca.idsucursal,
suc.sucursal,
enca.fechacompra,
enca.idproveedor,
prov.proveedor,
enca.idestado,
est.estado,
enca.codigofactura,
enca.seriefactura,
enca.idusuario,
enca.observacion,
deta.idproducto,
pro.producto,
deta.cantidadsolicitada,
deta.cantidadconfirmada,
deta.precio,
deta.iva


 FROM compra enca
LEFT OUTER JOIN
sucursal suc
ON suc.idsucursal = enca.idsucursal
LEFT OUTER JOIN
proveedor prov
ON prov.idproveedor = enca.idproveedor
LEFT OUTER JOIN
estado est
ON est.idestado = enca.idestado
LEFT OUTER JOIN
usuario usr
ON usr.idusuario = enca.idusuario

LEFT OUTER JOIN
compradetalle deta
ON enca.idcompra = deta.idcompra
AND enca.idsucursal =  deta.idsucursal

LEFT OUTER JOIN
producto pro 
ON pro.idproducto = deta.idproducto 
WHERE deta.cantidadconfirmada > 0
and enca.idcompra = $P{IDCOMPRA}
and enca.idsucursal = $P{CODSUC}]]>
	</queryString>
